pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Compilation du projet...'
            }
        }
        stage('Test') {
            steps {
                echo 'Exécution des tests...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Déploiement terminé !'
            }
        }
    }
}
